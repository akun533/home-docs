{
  "name": "home-docs",
  "version": "1.0.0",
  "description": "前后端一体化博客系统",
  "private": true,
  "workspaces": [
    "frontend",
    "server"
  ],
  "scripts": {
    "frontend:dev": "cd frontend && npm run docs:dev",
    "frontend:build": "cd frontend && npm run docs:build",
    "frontend:install": "cd frontend && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:install": "cd server && npm install",
    "install:all": "npm run frontend:install && npm run server:install",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run server:dev\"",
    "build": "npm run frontend:build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "docker-compose up -d --build"
  },
  "keywords": ["blog", "vuepress", "express", "fullstack"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
